<div class="container ">
    <form method="get" action="#" class="form-inline">
        <div class="row search-area">
            <div class="col-lg-10 col-md-10 ">
                <input type="text" class="form-control" name="keyword" placeholder="Search Doctor ... " id="searchkey" onkeyup="SearchStart()" autocomplete="off" style="height: 42px;width: 100%" />
            </div>
            <div class="col-lg-2 col-md-2">
                <button class="btn btn-block" style="margin-top:3px">Search</button>
            </div>
            <div class="search-result-area" id="searchresults">
            </div>
        </div>
    </form>
</div>
<script>
    var loader = "<ul class='list-group' style='position:absolute;margin-left: 13px;width: 97%; margin-top: 1px;border: 2px solid #d7d8d8;border-top: 0;'>" +
        "<li class='list-group-item'>" +

        "<center>" +
        "<img src='/Content/Images/Gif/loading.gif' />" +
        "</center>" +

        "</li>" +
        "</ul>";
    var count = 0;
    function SearchStart() {
        if (count == 0) {
            $("#searchresults").html(loader);

        }
        var key = $("#searchkey").val();
        $.ajax({

            url: "/Search/Suggestions?key=" + key,
            method: "get"
        }).done(function (results) {
            count++;
            $("#searchresults").html(results);

        });
    }
</script>
